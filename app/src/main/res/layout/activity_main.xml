<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="#efefef">

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true">
        
        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">
            
            
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="200dp"
                android:orientation="vertical"
                android:gravity="center"
                android:background="#242E3E">

                <ImageView
                    android:layout_width="140dp"
                    android:layout_height="140dp"
                    android:padding="10dp"
                    android:src="@drawable/profile"/>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Meterial deshboard"
                    android:textSize="35sp"
                    android:textColor="#ffffff"
                    android:layout_marginBottom="2dp"
                    android:layout_marginTop="5dp"/>


            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="210dp"
                android:orientation="vertical"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="170dp"
                    android:orientation="horizontal">
                    
                    <androidx.cardview.widget.CardView
                        android:layout_width="0dp"
                        android:layout_height="160dp"
                        android:layout_weight="1"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        app:cardCornerRadius="15dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <ImageView
                                android:id="@+id/profile"
                                android:layout_width="70dp"
                                android:layout_height="70dp"
                                android:src="@drawable/profile"
                                android:layout_centerInParent="true"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Profile"
                                android:textSize="18sp"
                                android:layout_centerInParent="true"
                                android:textColor="#000000"
                                android:layout_below="@+id/profile"
                                android:layout_marginTop="10dp"
                                android:textStyle="bold"/>
                            
                            <Button
                                android:id="@+id/btnprofile"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@android:color/transparent"/>


                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="0dp"
                        android:layout_height="160dp"
                        android:layout_weight="1"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        app:cardCornerRadius="15dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <ImageView
                                android:id="@+id/education"
                                android:layout_width="70dp"
                                android:layout_height="70dp"
                                android:src="@drawable/education"
                                android:layout_centerInParent="true"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Education"
                                android:textSize="18sp"
                                android:layout_centerInParent="true"
                                android:textColor="#000000"
                                android:layout_below="@+id/education"
                                android:layout_marginTop="10dp"
                                android:textStyle="bold"/>

                            <Button
                                android:id="@+id/btneducation"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@android:color/transparent"/>


                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>



                    


                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="170dp"
                    android:orientation="horizontal">

                    <androidx.cardview.widget.CardView
                        android:layout_width="0dp"
                        android:layout_height="160dp"
                        android:layout_weight="1"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        app:cardCornerRadius="15dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <ImageView
                                android:id="@+id/Love"
                                android:layout_width="70dp"
                                android:layout_height="70dp"
                                android:src="@drawable/love"
                                android:layout_centerInParent="true"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Love"
                                android:textSize="18sp"
                                android:layout_centerInParent="true"
                                android:textColor="#000000"
                                android:layout_below="@+id/Love"
                                android:layout_marginTop="10dp"
                                android:textStyle="bold"/>

                            <Button
                                android:id="@+id/btnlove"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@android:color/transparent"/>


                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="0dp"
                        android:layout_height="160dp"
                        android:layout_weight="1"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        app:cardCornerRadius="15dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <ImageView
                                android:id="@+id/goal"
                                android:layout_width="70dp"
                                android:layout_height="70dp"
                                android:src="@drawable/goal"
                                android:layout_centerInParent="true"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Goal"
                                android:textSize="18sp"
                                android:layout_centerInParent="true"
                                android:textColor="#000000"
                                android:layout_below="@+id/goal"
                                android:layout_marginTop="10dp"
                                android:textStyle="bold"/>

                            <Button
                                android:id="@+id/btngoal"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@android:color/transparent"/>


                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>






                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="170dp"
                    android:orientation="horizontal">

                    <androidx.cardview.widget.CardView
                        android:layout_width="0dp"
                        android:layout_height="160dp"
                        android:layout_weight="1"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        app:cardCornerRadius="15dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <ImageView
                                android:id="@+id/health"
                                android:layout_width="70dp"
                                android:layout_height="70dp"
                                android:src="@drawable/health"
                                android:layout_centerInParent="true"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Health"
                                android:textSize="18sp"
                                android:layout_centerInParent="true"
                                android:textColor="#000000"
                                android:layout_below="@+id/health"
                                android:layout_marginTop="10dp"
                                android:textStyle="bold"/>

                            <Button
                                android:id="@+id/btnhealth"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@android:color/transparent"/>


                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="0dp"
                        android:layout_height="160dp"
                        android:layout_weight="1"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        app:cardCornerRadius="15dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <ImageView
                                android:id="@+id/finance"
                                android:layout_width="70dp"
                                android:layout_height="70dp"
                                android:src="@drawable/finance"
                                android:layout_centerInParent="true"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="finance"
                                android:textSize="18sp"
                                android:layout_centerInParent="true"
                                android:textColor="#000000"
                                android:layout_below="@+id/finance"
                                android:layout_marginTop="10dp"
                                android:textStyle="bold"/>

                            <Button
                                android:id="@+id/btnfinance"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@android:color/transparent"/>


                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>






                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="170dp"
                    android:orientation="horizontal">

                    <androidx.cardview.widget.CardView
                        android:layout_width="0dp"
                        android:layout_height="160dp"
                        android:layout_weight="1"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        app:cardCornerRadius="15dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <ImageView
                                android:id="@+id/Helpline"
                                android:layout_width="70dp"
                                android:layout_height="70dp"
                                android:src="@drawable/help"
                                android:layout_centerInParent="true"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Help line"
                                android:textSize="18sp"
                                android:layout_centerInParent="true"
                                android:textColor="#000000"
                                android:layout_below="@+id/Helpline"
                                android:layout_marginTop="10dp"
                                android:textStyle="bold"/>

                            <Button
                                android:id="@+id/btnHelpline"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@android:color/transparent"/>


                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="0dp"
                        android:layout_height="160dp"
                        android:layout_weight="1"
                        android:layout_marginLeft="8dp"
                        android:layout_marginRight="8dp"
                        app:cardCornerRadius="15dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent">

                            <ImageView
                                android:id="@+id/Computer"
                                android:layout_width="70dp"
                                android:layout_height="70dp"
                                android:src="@drawable/computer"
                                android:layout_centerInParent="true"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Computer"
                                android:textSize="18sp"
                                android:layout_centerInParent="true"
                                android:textColor="#000000"
                                android:layout_below="@+id/Computer"
                                android:layout_marginTop="10dp"
                                android:textStyle="bold"/>

                            <Button
                                android:id="@+id/btnIComputer"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:background="@android:color/transparent"/>


                        </RelativeLayout>

                    </androidx.cardview.widget.CardView>






                </LinearLayout>











            </LinearLayout>

        </RelativeLayout>
        
    </ScrollView>
</RelativeLayout>